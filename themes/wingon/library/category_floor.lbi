<script type="text/javascript">

if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
	$('img.lazy').each(function(){
      $(this).attr('src',$(this).data('original'));
   });

}
else{
	$(function() {
		$("img.lazy").lazyload();
	});
}
</script>