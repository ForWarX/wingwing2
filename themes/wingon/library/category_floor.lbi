    <!--{foreach from=$cat_info.top_7 key=key item=good}-->
    	<div class="cat_floor_gap" style="background-color:{$cat_info.theme_color}"></div>
        <!--{if $key eq 0}--><span class="acc-trigger active"><!--{else}--><span class="acc-trigger"><!--{/if}--><a style="background-color:{$cat_info.theme_color}"><span class="f_l" style="widtH:125px;"><!--{if $good.is_shipping}--><span style="position:absolute;left:5px;color:#fff;background-color:#f23622;padding:0 5px 3px 2px;height:29px;line-height:15px;top:0;width:10px;">包邮</span> <!--{/if}-->{$good.index_short_name}</span></a></span>
        <div class="acc-container">
        	<div class="trapezoid" style="border-color:transparent transparent {$cat_info.theme_color}"></div>
        	<a href="{$good.url}" title="{$good.name}"><div style="margin-left:10px;margin-top:8px;">
            	<ul>
                <li><span><img src="{$good.thumb}" width="60"/></span></li>
            	<li><span class="promo_price"><!--{if $good.promote_price}-->{$good.promote_price}<!--{else}-->{$good.shop_price}<!--{/if}--></span></li>
                </ul>
            </div></a>
        </div>
        <!--{/foreach}-->
      <!-- end- accordion -->
</div>
<div class="promo_main">
	<div style="width:333px;height:333px;float:left; overflow:hidden;display:block;">
    <div id="category_slide_container_{$cat_info.cat_id}" style="position: relative; margin: 0 auto;
        top: 0px; left: 0px; width: 333px; height: 333px; overflow: hidden;">
        
        <div u="loading" style="position: absolute; top: 0px; left: 0px;">
            <div style="filter: alpha(opacity=70); opacity: 0.7; position: absolute; display: block;
                top: 0px; left: 0px; width: 100%; height: 100%;">
            </div>
            <div style="position: absolute; display: block; background: url(images/loading.gif) no-repeat center center;
                top: 0px; left: 0px; width: 100%; height: 100%;">
            </div>
        </div>
        
        <div u="slides"  class="box_shadow" style="cursor: move; position: absolute; left: 0px; top: 0px; width: 333px;height: 333px; overflow: hidden;">
        	<!--{foreach from=$cat_info.banner key=key item=banner}-->
        	<div><a href="{$banner.url}" target="_blank"><img u="image" src="{$banner.src}"/></a></div>
            <!--{/foreach}-->
            
        </div>
                
        <style>
            .jssorb21 div, .jssorb21 div:hover, .jssorb21 .av
            {
                background: url(images/b21.png) no-repeat;
                overflow:hidden;
                cursor: pointer;
            }
            /*.jssorb21 div {background-position: -5px -5px;}
            .jssorb21 div:hover, .jssorb21 .av:hover {background-position: -35px -5px;}*/
            .jssorb21 .av {background-position: -65px -5px;}
            .jssorb21 .dn, .jssorb21 .dn:hover {background-position: -95px -5px;}
        </style>
        

        
        <style>
            /* jssor slider arrow navigator skin 21 css */
            /*
            .jssora21l              (normal)
            .jssora21r              (normal)
            .jssora21l:hover        (normal mouseover)
            .jssora21r:hover        (normal mouseover)
            .jssora21ldn            (mousedown)
            .jssora21rdn            (mousedown)
            */
            .jssora21l, .jssora21r, .jssora21ldn, .jssora21rdn
            {
            	position: absolute;
            	cursor: pointer;
            	display: block;
                background: url(images/a21.png) center center no-repeat;
                overflow: hidden;
            }
            .jssora21l {position: absolute;background-position: -3px -33px;}
            .jssora21r {position: absolute;background-position: -63px -33px;}
            .jssora21l:hover {background-position: -3px -33px;}
            .jssora21r:hover {background-position: -63px -33px;}
            .jssora21ldn {background-position: -3px -33px;}
            .jssora21rdn {background-position: -63px -33px;}
			/*.jssora21l:hover {background-position: -123px -33px;}
            .jssora21r:hover {background-position: -183px -33px;}
            .jssora21ldn {background-position: -243px -33px;}
            .jssora21rdn {background-position: -303px -33px;}*/
        </style>
        
        <span u="arrowleft" class="jssora21l" style="width: 55px; height: 55px; top: 123px; left: 8px;">
        </span>
        
        <span u="arrowright" class="jssora21r" style="width: 55px; height: 55px; top: 123px; right: 8px">
        </span>
        
  
    </div>
    
    <script>
        jssor_category_slide_starter('category_slide_container_{$cat_info.cat_id}');
    </script>
        
        
    </div>
    <div class="f_r" style="width:690px;position:relative;">
        <div style="position:absolute;height:333px;width:10px;background:#fff;left:0;z-index:99;"></div>
    	<ul class="sis-li">
        <!--{foreach from=$cat_info.rank815 key=key item=good}-->
        	<li>
            	<span style="height:5px;display:block"> </span>
            	<div style="width:148px;border-left:1px dashed rgba(117,117,117,0.3);margin-bottom: 3px;position:relative;">
                <a href="{$good.url}" title="{$good.name}"><div style="width:110px;">
            	<img style="opacity: 1;" class="lazy item_img" width="110" data-original="{$good.thumb}">
                <!--{if $good.is_shipping eq 1}--><span style="position:absolute;right:5px;top:0;color:#fff;background:#f23622;padding:0 5px;">包邮</span><!--{/if}-->
                <span style="display:block;width:120px;">{$good.index_short_name}</span>
                </div></a>
                </div>
                <span style="display:block;text-align:center;font-size:1.7em;color:#f23622;"><!--{if $good.promote_price}-->{$good.promote_price}<!--{else}-->{$good.shop_price}<!--{/if}--></span>
            </li>
         <!--{/foreach}-->
        </ul>
    </div>
</div>
</div>
